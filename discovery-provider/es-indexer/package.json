{
  "name": "es-indexer",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "graphql:dev": "run-p graphql:dev:codegen graphql:dev:server",
    "graphql:dev:codegen": "graphql-codegen --watch --config gqlcodegen.yml",
    "graphql:dev:server": "ts-node-dev --respawn --transpile-only gqlapi/server.ts",
    "graphql:start": "pm2-runtime build/gqlapi/server.js --restart-delay=3000",
    "build": "tsc",
    "catchup": "node build/src/main.js --no-listen",
    "catchup:ci": "ts-node --transpileOnly src/main.ts --drop --no-listen",
    "dev": "ts-node src/main.ts",
    "nuke": "ts-node scripts/nuke.ts",
    "sql-ts": "ts-node scripts/sql-ts.ts && pg_dump -s --no-owner --no-privileges $audius_db_url > tests/schema.sql",
    "start": "pm2-runtime start ecosystem.config.js",
    "test": "vitest",
    "tsc": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "devDependencies": {
    "@graphql-codegen/cli": "2.11.6",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-resolvers": "2.7.3",
    "@rmp135/sql-ts": "^1.13.0",
    "@types/lodash": "4.14.182",
    "@types/node": "17.0.29",
    "@types/pg": "8.6.5",
    "@types/pg-cursor": "^2.7.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "10.7.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "3.14.1",
    "typescript": "4.6.3",
    "vitest": "^0.24.1"
  },
  "dependencies": {
    "@elastic/elasticsearch": "8.1.0",
    "@types/commander": "^2.12.2",
    "apollo-server": "^3.10.0",
    "bodybuilder": "^2.5.0",
    "commander": "9.2.0",
    "dataloader": "^2.1.0",
    "graphql": "^16.5.0",
    "hashids": "^2.2.10",
    "knex": "^2.1.0",
    "lodash": "4.17.21",
    "pg": "8.7.3",
    "pg-cursor": "2.7.3",
    "pino": "7.11.0",
    "pm2": "5.2.0"
  }
}
