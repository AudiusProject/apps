    "@types/node": "17.0.29",
    "@types/pg": "8.6.5",
    "inquirer": "^9.1.4",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.4",
    "ts-node": "10.7.0",
    "tsconfig-paths": "3.14.1",
    "typescript": "4.6.3"
  },
  "dependencies": {
    "@aws-sdk/client-sns": "^3.245.0",
    "commander": "9.2.0",
    "graphile-worker": "^0.13.0",
