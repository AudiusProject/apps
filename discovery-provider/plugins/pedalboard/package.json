{
  "name": "with-docker",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev --concurrency 12",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test"
  },
  "dependencies": {
    "basekit": "^0.0.0",
    "knex": "^2.4.2",
    "storage": "^0.0.0",
    "verified-events": "^0.0.0"
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "^2.8.8",
    "turbo": "^1.9.3"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "packageManager": "npm@9.6.6",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
