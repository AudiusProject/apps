{
  "name": "create-audius-app",
  "version": "1.0.2",
  "description": "Create an Audius app with one command",
  "scripts": {
    "build": "tsup ./index.ts",
    "dev": "tsup ./index.ts --watch",
    "lint": "eslint",
    "prepublishOnly": "rm -rf dist && npm run build",
    "setup-test-env": "./tests/e2e/setup-test-env.sh",
    "test": "vitest tests/integration",
    "test:e2e": "playwright test tests/e2e",
    "typecheck": "tsc",
    "verify": "concurrently \"npm:typecheck\" \"npm:lint\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AudiusProject/audius-protocol.git",
    "directory": "packages/create-audius-app"
  },
  "author": "Tiki Labs",
  "keywords": [
    "audius",
    "music",
    "decentralized"
  ],
  "license": "MIT",
  "bin": {
    "create-audius-app": "./dist/index.js"
  },
  "files": [
    "dist",
    "examples"
  ],
  "devDependencies": {
    "@playwright/test": "1.42.1",
    "@types/prompts": "2.4.2",
    "@types/tar": "6.1.11",
    "@types/validate-npm-package-name": "4.0.2",
    "commander": "2.20.0",
    "execa": "2.0.3",
    "fast-glob": "3.3.1",
    "prompts": "2.4.2",
    "tar": "6.2.0",
    "tsup": "8.0.2",
    "validate-npm-package-name": "5.0.0",
    "vitest": "1.3.0"
  }
}
