{
  "name": "@audius/ddex-entrypoint",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "start:dev": "env-cmd -f .env.dev npm run start",
    "start:stage": "env-cmd -f .env.stage npm run start",
    "start:prod": "env-cmd -f .env.prod npm run start",
    "build": "tsc && vite build",
    "build:dev": "env-cmd -f .env.dev npm run build",
    "build:stage": "env-cmd -f .env.stage npm run build",
    "build:prod": "env-cmd -f .env.prod npm run build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "deploy:stage": "npm run build:stage && wrangler pages deploy --branch=stage ./dist",
    "deploy:prod": "npm run build:prod && wrangler pages deploy ./dist"
  },
  "dependencies": {
    "@audius/harmony": "*",
    "@audius/sdk": "*",
    "@tanstack/react-query": "5.62.7",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "0.2.3",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "@vitejs/plugin-react": "4.0.3",
    "env-cmd": "10.1.0",
    "eslint": "8.56.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.3",
    "vite-plugin-node-polyfills": "0.17.0",
    "typescript": "5.0.2",
    "vite": "4.4.5",
    "wrangler": "3.30.1"
  }
}
