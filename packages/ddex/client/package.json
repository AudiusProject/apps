{
  "name": "@audius/ddex-client",
  "private": true,
  "version": "0.0.3",
  "type": "module",
  "scripts": {
    "start": "vite",
    "start:dev": "IS_DEV=true env-cmd -f .env.dev turbo run start --filter=@audius/ddex-client",
    "start:stage": "IS_DEV=true env-cmd -f .env.stage.local turbo run start --filter=@audius/ddex-client",
    "start:prod": "IS_DEV=true env-cmd -f .env.prod.local turbo run start --filter=@audius/ddex-client",
    "stage:preview": "IS_DEV=true env-cmd -f .env.stage.local vite build && vite preview",
    "prod:preview": "IS_DEV=true env-cmd -f .env.prod.local vite build && vite preview",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --fix src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc",
    "verify": "concurrently \"npm:typecheck\" \"npm:lint\""
  },
  "dependencies": {
    "@audius/harmony": "*",
    "@audius/sdk": "*",
    "@emotion/react": "11.11.3",
    "@emotion/styled": "11.11.0",
    "@headlessui/react": "1.7.17",
    "@heroicons/react": "1.0.6",
    "@optimizely/optimizely-sdk": "4.9.2",
    "@tanstack/query-core": "5.8.7",
    "@tanstack/react-query": "5.8.4",
    "@tanstack/react-query-devtools": "5.8.4",
    "@trpc/client": "next",
    "@trpc/react-query": "next",
    "@trpc/server": "next",
    "autoprefixer": "10.4.16",
    "@radix-ui/react-slot": "1.0.2",
    "@react-spring/web": "9.7.3",
    "classnames": "2.5.1",
    "hashids": "2.2.1",
    "linkedom": "0.15.6",
    "lodash": "4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.20.0",
    "react-lottie": "1.2.3",
    "react-merge-refs": "2.1.1",
    "react-perfect-scrollbar": "1.5.8",
    "react-use": "17.4.2",
    "react-use-measure": "2.1.1"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "5.8.4",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "@typescript-eslint/eslint-plugin": "6.14.0",
    "@typescript-eslint/parser": "6.14.0",
    "@vitejs/plugin-react-swc": "3.5.0",
    "env-cmd": "10.1.0",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.5",
    "prettier": "3.1.0",
    "typescript": "5.2.2",
    "vite": "4.4.5",
    "vite-plugin-node-polyfills": "0.16.0",
    "vite-tsconfig-paths": "4.2.2"
  }
}
