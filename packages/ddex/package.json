{
  "name": "@audius/ddex",
  "private": true,
  "version": "0.0.2",
  "type": "module",
  "scripts": {
    "start": "vite",
    "start:dev": "env-cmd -f .env.dev turbo run start --filter=@audius/ddex",
    "start:stage": "env-cmd -f .env.stage.local turbo run start --filter=@audius/ddex",
    "start:prod": "env-cmd -f .env.prod.local turbo run start --filter=@audius/ddex",
    "stage:preview": "env-cmd -f .env.stage.local vite build && vite preview",
    "prod:preview": "env-cmd -f .env.prod.local vite build && vite preview",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --fix src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc",
    "verify": "concurrently \"npm:typecheck\" \"npm:lint\""
  },
  "dependencies": {
    "@audius/sdk": "*",
    "@headlessui/react": "1.7.17",
    "@heroicons/react": "1.0.6",
    "@tanstack/query-core": "5.8.7",
    "@tanstack/react-query": "5.8.4",
    "@tanstack/react-query-devtools": "5.8.4",
    "autoprefixer": "10.4.16",
    "linkedom": "0.15.6",
    "postcss": "8.4.31",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.20.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "0.5.7",
    "@tanstack/eslint-plugin-query": "5.8.4",
    "@types/react": "18.2.43",
    "@types/react-dom": "18.2.17",
    "@typescript-eslint/eslint-plugin": "6.14.0",
    "@typescript-eslint/parser": "6.14.0",
    "@vitejs/plugin-react-swc": "3.5.0",
    "env-cmd": "10.1.0",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.5",
    "prettier": "3.1.0",
    "tailwindcss": "3.3.5",
    "typescript": "5.2.2",
    "vite": "4.4.5",
    "vite-plugin-node-polyfills": "0.16.0",
    "vite-tsconfig-paths": "4.2.2"
  }
}
