{
  "name": "@audius/ddex-publisher",
  "version": "0.0.12",
  "description": "Server that publishes DDEX entities queued for release",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:dev": "nodemon src/index.ts",
    "dev:dev": "turbo run start:dev --filter=@audius/ddex-publisher",
    "dev:stage": "NETWORK=stage turbo run start:dev --filter=@audius/ddex-publisher",
    "dev:prod": "NETWORK=prod turbo run start:dev --filter=@audius/ddex-publisher",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --fix src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "verify": "concurrently \"npm:build\" \"npm:lint\""
  },
  "keywords": [],
  "author": "Audius",
  "dependencies": {
    "@audius/sdk": "*",
    "@aws-sdk/client-s3": "3.504.0",
    "dotenv": "16.3.1",
    "mongoose": "8.1.0",
    "web3": "4.3.0"
  },
  "devDependencies": {
    "@types/node": "20.11.10",
    "@typescript-eslint/eslint-plugin": "6.17.0",
    "@typescript-eslint/parser": "6.17.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.2",
    "typescript": "5.3.3"
  }
}
