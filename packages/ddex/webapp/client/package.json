{
  "name": "@audius/ddex-webapp-client",
  "private": true,
  "version": "0.0.9",
  "type": "module",
  "scripts": {
    "start": "vite",
    "start:dev": "IS_DEV=true env-cmd -f .env.dev turbo run start --filter=@audius/ddex-webapp-client",
    "start:stage": "IS_DEV=true env-cmd -f .env.stage.local turbo run start --filter=@audius/ddex-webapp-client",
    "start:prod": "IS_DEV=true env-cmd -f .env.prod.local turbo run start --filter=@audius/ddex-webapp-client",
    "stage:preview": "IS_DEV=true env-cmd -f .env.stage.local vite build && vite preview",
    "prod:preview": "IS_DEV=true env-cmd -f .env.prod.local vite build && vite preview",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --fix src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc",
    "verify": "concurrently \"npm:typecheck\" \"npm:lint\""
  },
  "dependencies": {
    "@audius/harmony": "*",
    "@audius/sdk": "*",
    "@emotion/css": "11.11.2",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@optimizely/optimizely-sdk": "4.9.2",
    "@radix-ui/react-slot": "1.0.2",
    "@react-spring/web": "9.7.3",
    "@tanstack/react-query": "4.35.7",
    "@trpc/client": "10.38.4",
    "@trpc/react-query": "10.38.4",
    "@trpc/server": "10.38.4",
    "classnames": "2.5.1",
    "hashids": "2.2.1",
    "lodash": "4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-lottie": "1.2.3",
    "react-merge-refs": "2.1.1",
    "react-perfect-scrollbar": "1.5.8",
    "react-router-dom": "6.20.0",
    "react-spring": "8.0.27",
    "react-use": "17.4.2",
    "react-use-measure": "2.1.1",
    "web3": "4.3.0"
  },
  "devDependencies": {
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "@vitejs/plugin-react-swc": "3.5.0",
    "env-cmd": "10.1.0",
    "eslint": "8.55.0",
    "eslint-plugin-react-refresh": "0.4.5",
    "typescript": "5.0.4",
    "vite": "4.4.5",
    "vite-plugin-node-polyfills": "0.16.0",
    "vite-tsconfig-paths": "4.2.2"
  }
}
