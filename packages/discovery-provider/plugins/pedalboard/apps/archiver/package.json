{
  "name": "@pedalboard/archiver",
  "version": "0.0.1",
  "description": "Creates archive files of tracks for a given entity",
  "private": true,
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "ts-node-dev --respawn ./src/index.ts",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\"",
    "start": "node ./dist/index.js"
  },
  "dependencies": {
    "@pedalboard/basekit": "*",
    "@pedalboard/logger": "*",
    "@pedalboard/storage": "*",
    "body-parser": "^1.19.0",
    "bullmq": "5.41.7",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "redis": "4.6.11"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.22",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.12.2",
    "@types/supertest": "^2.0.11",
    "esbuild": "^0.14.38",
    "esbuild-register": "^3.3.2",
    "eslint": "8.56.0",
    "eslint-config-custom-server": "*",
    "jest": "^26.6.3",
    "jest-presets": "*",
    "tsconfig": "*",
    "typescript": "^4.5.3"
  }
}
