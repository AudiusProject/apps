{
  "name": "@pedalboard/mri",
  "version": "0.1.0",
  "description": "plugin for publishing statistics to mri",
  "main": "src/index.ts",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "watch": "tsc --watch",
    "serve": "nodemon",
    "dev": "concurrently \"npm run watch\" \"npm run serve\"",
    "test:local": "docker compose up -d && chmod +x ./scripts/seed-prod-db.sh && ./scripts/seed-prod-db.sh",
    "test": "vitest",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\""
  },
  "author": "alec@audius.co",
  "license": "ISC",
  "devDependencies": {
    "@pedalboard/logger": "*",
    "@pedalboard/storage": "^0.0.30",
    "concurrently": "^8.2.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.577.0",
    "@types/cron": "2.4.0",
    "cron": "3.1.7",
    "envalid": "8.0.0",
    "knex": "3.1.0",
    "pg": "8.11.5"
  }
}
