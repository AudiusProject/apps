    "express": "4.17.1",
    "morgan": "1.10.0",
    "pino": "8.16.1",
    "redis": "4.6.11"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.0",
    "@types/cors": "2.8.10",
    "@types/express": "4.17.12",
    "@types/jest": "26.0.22",
    "@types/morgan": "1.9.2",
    "@types/node": "15.12.2",
    "@types/supertest": "2.0.11",
    "esbuild": "0.14.38",
    "esbuild-register": "3.3.2",
    "eslint": "7.32.0",
    "eslint-config-custom-server": "*",
