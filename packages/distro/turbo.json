{
  "extends": ["//"],
  "tasks": {
    "build": {
      "outputs": ["dist/**"],
      "dependsOn": ["@audius/harmony#build", "@audius/sdk#build:browser"],
      "outputLogs": "new-only"
    },
    "start": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["@audius/harmony#build", "@audius/sdk#build:browser"],
      "outputLogs": "new-only"
    },
    "typecheck": {
      "outputLogs": "new-only"
    },
    "verify": {
      "outputLogs": "new-only"
    }
  }
}
