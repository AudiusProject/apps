{
  "name": "embed",
  "version": "1.5.51",
  "scripts": {
    "start": "vite",
    "start:dev": "env-cmd -f .env.dev npm run -s start",
    "start:stage": "env-cmd -f .env.stage npm run -s start",
    "start:prod": "env-cmd -f .env.prod npm run -s start",
    "build": "vite build",
    "build:stage": "env-cmd -f .env.stage npm run build && npm run build-api && mv build build-staging",
    "build:prod": "env-cmd -f .env.prod npm run build && npm run build-api && mv build build-production",
    "build-api": "webpack --config src/api/webpack.config.js -o build/api.js",
    "lint:fix": "eslint --cache --fix --ext=js,jsx,ts,tsx src",
    "lint": "eslint --cache --ext=js,jsx,ts,tsx src"
  },
  "devDependencies": {
    "@cloudflare/kv-asset-handler": "^0.3.0",
    "@vitejs/plugin-react": "4.1.0",
    "env-cmd": "10.1.0",
    "eslint": "8.25.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.4.2",
    "per-env": "1.0.2",
    "serve": "11.1.0",
    "vite": "4.5.0",
    "vite-plugin-glslify": "2.0.2",
    "vite-plugin-svgr": "4.1.0",
    "vite-tsconfig-paths": "4.2.1",
    "webpack-cli": "4.5.0"
  },
  "dependencies": {
    "@audius/harmony": "*",
    "@audius/sdk": "*",
    "@audius/stems": "*",
    "amplitude-js": "8.11.1",
    "axios": "0.19.2",
    "classnames": "2.2.6",
    "copy-to-clipboard": "3.2.1",
    "hashids": "2.2.10",
    "hls.js": "0.13.1",
    "node-polyfill-webpack-plugin": "2.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "react-router-dom": "6.18.0",
    "react-transition-group": "4.4.5",
    "simplebar-react": "2.1.0",
    "web3": "1.8.2",
    "web3-utils": "1.3.4"
  }
}
