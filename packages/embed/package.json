{
  "name": "embed",
  "version": "1.5.71",
  "private": true,
  "scripts": {
    "start": "vite",
    "start:dev": "env-cmd -f .env.dev turbo run start",
    "start:stage": "env-cmd -f .env.stage turbo run start",
    "start:prod": "env-cmd -f .env.prod turbo run start",
    "build": "vite build",
    "build:stage": "env-cmd -f .env.stage turbo run build && npm run build-api && mv build build-staging",
    "build:prod": "env-cmd -f .env.prod turbo run build && npm run build-api && mv build build-production",
    "build-api": "webpack --config src/api/webpack.config.js -o build/api.js",
    "lint:fix": "eslint --cache --fix --ext=js,jsx,ts,tsx src",
    "lint": "eslint --cache --ext=js,jsx,ts,tsx src",
    "lint:env": "dotenv-linter",
    "verify": "concurrently \"npm:lint:fix\" \"npm:lint:env\""
  },
  "devDependencies": {
    "@audius/dotenv-linter": "*",
    "@cloudflare/kv-asset-handler": "^0.3.0",
    "@vitejs/plugin-react": "4.1.0",
    "env-cmd": "10.1.0",
    "eslint": "8.25.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.4.2",
    "per-env": "1.0.2",
    "serve": "11.1.0",
    "vite": "4.5.0",
    "vite-plugin-glslify": "2.0.2",
    "vite-plugin-svgr": "4.1.0",
    "vite-tsconfig-paths": "4.2.1",
    "webpack-cli": "4.5.0"
  },
  "dependencies": {
    "@audius/fetch-nft": "0.1.8",
    "@audius/fixed-decimal": "*",
    "@audius/harmony": "*",
    "@audius/sdk": "*",
    "@audius/stems": "1.5.56",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@google/model-viewer": "3.3.0",
    "@radix-ui/react-slot": "1.0.2",
    "@react-spring/web": "9.7.3",
    "amplitude-js": "8.11.1",
    "axios": "0.19.2",
    "classnames": "2.2.6",
    "copy-to-clipboard": "3.2.1",
    "hashids": "2.2.10",
    "hls.js": "0.13.1",
    "lodash": "^4.17.21",
    "node-polyfill-webpack-plugin": "2.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "react-lottie": "1.2.3",
    "react-merge-refs": "2.1.1",
    "react-perfect-scrollbar": "1.5.8",
    "react-router-dom": "6.18.0",
    "react-transition-group": "4.4.5",
    "react-use": "17.4.0",
    "react-use-measure": "2.1.1",
    "simplebar-react": "2.1.0",
    "web3": "1.8.2",
    "web3-utils": "1.3.4"
  }
}
