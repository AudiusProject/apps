{
  "extends": ["//"],
  "tasks": {
    "build": {
      "outputs": ["build/**"],
      "dependsOn": [
        "@audius/common#build",
        "@audius/fixed-decimal#build",
        "@audius/harmony#build",
        "@audius/sdk#build:browser"
      ],
      "outputLogs": "new-only"
    },
    "start": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "@audius/common#build",
        "@audius/fixed-decimal#build",
        "@audius/harmony#build",
        "@audius/sdk#build:browser"
      ],
      "outputLogs": "new-only"
    },
    "typecheck": {
      "outputLogs": "new-only"
    },
    "verify": {
      "outputLogs": "new-only"
    }
  }
}
