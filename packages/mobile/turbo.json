{
  "extends": ["//"],
  "pipeline": {
    "android": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true,
      "outputMode": "new-only"
    },
    "ios": {
      "dependsOn": ["^build", "install-pods"],
      "cache": false,
      "persistent": true,
      "outputMode": "new-only"
    },
    "bundle:ios": {
      "outputs": ["./ios/main.jsbundle"],
      "dependsOn": ["^build"],
      "outputMode": "new-only"
    },
    "install-pods": {
      "outputs": [],
      "inputs": ["../../package-lock.json"]
    }
  }
}
