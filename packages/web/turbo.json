{
  "extends": ["//"],
  "pipeline": {
    "build": {
      "outputs": [
        "build-development/**",
        "build-staging/**",
        "build-production/**"
      ],
      "dependsOn": ["@audius/sdk#build:web", "^build"],
      "outputMode": "new-only"
    },
    "dist": {
      "outputs": ["dist/**"],
      "dependsOn": ["@audius/sdk#build:web", "^build"],
      "outputMode": "new-only"
    },
    "start": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["@audius/sdk#build:web", "^build"],
      "outputMode": "new-only"
    },
    "lint": {
      "outputMode": "new-only",
      "dependsOn": ["@audius/sdk#build:web", "^build"]
    },
    "lint:fix": {
      "outputMode": "new-only",
      "dependsOn": ["@audius/sdk#build:web", "^build"]
    },
    "verify": {
      "outputMode": "new-only",
      "dependsOn": ["@audius/sdk#build:web", "^build"]
    }
  }
}
