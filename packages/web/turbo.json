{
  "extends": ["//"],
  "pipeline": {
    "build": {
      "outputs": ["build/**", "build-ssr/**"],
      "dependsOn": ["^build"],
      "outputMode": "new-only",
      "env": ["VITE_ENVIRONMENT", "VITE_SSR"]
    },
    "dist": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"],
      "outputMode": "new-only"
    }
  }
}
