{
  "name": "audius-protocol-dashboard",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "homepage": "./",
  "dependencies": {
    "@apollo/client": "3.3.7",
    "@audius/sdk": "3.0.11-beta.9",
    "@audius/stems": "0.3.10",
    "@reduxjs/toolkit": "1.4.0",
    "@self.id/3box-legacy": "0.3.0",
    "@self.id/core": "0.4.1",
    "chart.js": "2.9.3",
    "clsx": "1.1.1",
    "connected-react-router": "6.8.0",
    "dayjs": "1.8.36",
    "graphql": "15.5.0",
    "history": "4.7.2",
    "identicon.js": "2.3.3",
    "lodash": "4.17.20",
    "numeral": "2.0.6",
    "react": "17.0.2",
    "react-chartjs-2": "2.10.0",
    "react-country-flag": "3.0.2",
    "react-custom-scrollbars": "4.2.1",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-lottie": "1.2.3",
    "react-markdown": "5.0.3",
    "react-redux": "7.2.1",
    "react-router-dom": "5.2.0",
    "react-spinners": "0.5.12",
    "react-spring": "8.0.25",
    "react-virtualized": "9.22.2",
    "redux-thunk": "2.3.0",
    "remark-gfm": "1.0.0",
    "semver": "6.3.0",
    "simplebar-react": "3.0.0-beta.5",
    "url-join": "4.0.1",
    "vite": "4.4.9"
  },
  "scripts": {
    "publish-scripts": "./scripts/publishScripts.sh",
    "configure-local-env": "node ./scripts/configureEnv.cjs dev",
    "configure-stage-env": "node ./scripts/configureEnv.cjs stage",
    "configure-prod-env": "node ./scripts/configureEnv.cjs prod",
    "start": "vite",
    "serve": "vite preview",
    "start:dev:cloud": "npm run configure-local-env remote && ./node_modules/.bin/env-cmd -f .env.dev.local npm start",
    "start:dev": "npm run configure-local-env && ./node_modules/.bin/env-cmd -f .env.dev.local npm start",
    "start:stage": "npm run configure-stage-env && ./node_modules/.bin/env-cmd -f .env.stage.local npm start",
    "start:prod": "npm run configure-prod-env && ./node_modules/.bin/env-cmd -f .env.prod.local npm start",
    "build": "NODE_OPTIONS=--max_old_space_size=8192 tsc -p ./tsconfig.node.json && vite build",
    "build:stage": "npm run configure-stage-env && ./node_modules/.bin/env-cmd -f .env.stage.local npm run build && node ./scripts/rerouteLegacy.cjs stage",
    "build:prod": "npm run configure-prod-env && ./node_modules/.bin/env-cmd -f .env.prod.local npm run build && node ./scripts/rerouteLegacy.cjs",
    "lint": "npm run prettier:check",
    "lint:fix": "npm run prettier:write",
    "prettier:base": "prettier --parser typescript --single-quote --no-semi",
    "prettier:check": "npm run prettier:base -- --list-different \"src/**/*.{ts,tsx}\"",
    "prettier:write": "npm run prettier:base -- --write \"src/**/*.{ts,tsx}\"",
    "pull-dev-accounts": "node ./scripts/pullDevAccounts.js",
    "advance-blocks": "node ./scripts/advanceBlocks.js",
    "update-build:dev": "node ./scripts/updateBuild.cjs dev",
    "update-build:stage": "node ./scripts/updateBuild.cjs stage",
    "update-build:prod": "node ./scripts/updateBuild.cjs prod"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@openzeppelin/test-helpers": "0.5.6",
    "@pinata/sdk": "1.1.13",
    "@types/3box": "1.22.0",
    "@types/bn.js": "5.1.2",
    "@types/classnames": "2.2.10",
    "@types/history": "5.0.0",
    "@types/jest": "24.9.1",
    "@types/lodash": "4.14.160",
    "@types/node": "12.0.10",
    "@types/numeral": "0.0.28",
    "@types/react": "17.0.2",
    "@types/react-dom": "16.8.4",
    "@types/react-helmet": "5.0.16",
    "@types/react-lottie": "1.2.5",
    "@types/react-redux": "7.1.9",
    "@types/react-router-dom": "5.1.5",
    "@types/react-tooltip": "3.11.0",
    "@types/react-virtualized": "9.21.10",
    "@types/semver": "6.2.1",
    "@types/url-join": "4.0.0",
    "@vitejs/plugin-react-swc": "3.4.0",
    "armada-cli": "0.2.4",
    "assert": "2.0.0",
    "browserify-zlib": "0.2.0",
    "constants-browserify": "1.0.0",
    "crypto-browserify": "3.12.0",
    "env-cmd": "9.0.3",
    "https-browserify": "1.0.0",
    "jsdom": "22.1.0",
    "node-fetch": "2.6.1",
    "os-browserify": "0.3.0",
    "path-browserify": "1.0.1",
    "postcss": "8.4.14",
    "prettier": "1.19.1",
    "redux-devtools-extension": "2.13.8",
    "setimmediate": "1.0.5",
    "stream-browserify": "3.0.0",
    "stream-http": "3.2.0",
    "tls": "0.0.1",
    "tls-browserify": "0.2.2",
    "ts-loader": "9.4.4",
    "typescript": "5.2.2",
    "url": "0.11.0",
    "util": "0.12.4",
    "vite-plugin-node-polyfills": "^0.15.0",
    "vite-plugin-svgr": "^4.1.0",
    "vite-plugin-top-level-await": "^1.3.1",
    "vite-plugin-wasm": "^3.2.2",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "resolutions": {
    "bn.js": "5.1.2"
  }
}
