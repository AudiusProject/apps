  },
  "eth-contracts-predeployed": {
    "protocol": "http",
    "host": "localhost",
    "port": 8546,
    "health_check_endpoint": "",
    "up": [
      "audius-compose up eth-ganache"
    ],
    "down": [
      "docker compose rm -s -f eth-ganache"
